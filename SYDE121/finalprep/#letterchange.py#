def LetterChanges(str): 

  

   str = str.replace("a", "b")
   str.replace("b", "c")
   str.replace("c", "d")
   str.replace("d", "E")
   str.replace("e", "f")
   str.replace("f", "g")
   str.replace("g", "h")
   str.replace("h", "I")
   str.replace("i", "j")
   str.replace("j", "k")
   str.replace("k", "l")
   str.replace("l", "m")
   str.replace("m", "n")
   str.replace("n", "O")
   str.replace("o", "p")
   str.replace("p", "q")
   str.replace("q", "r")
   str.replace("r", "s")
   str.replace("s", "t")
   str.replace("t", "U")
   str.replace("u", "v")
   str.replace("v", "w")
   str.replace("w", "x")
   str.replace("x", "y")
   str.replace("y", "z")
   str.replace("z", "A")
   return str

print LetterChanges("Hello World abcd")
